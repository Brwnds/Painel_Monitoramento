ackend\n        const results = response.data;\n        this.panels = results.map(result => ({\n          link: result.link,\n          painel: result.painel,  // Nome do painel que veio do backend\n          status: result.status\n        }));\n        this.updatePanelCounts();  // Atualiza contagem de online/offline\n      } catch (error) {\n        console.error(\"Erro ao buscar dados dos painéis:\", error);\n      }\n    },\n    updatePanelCounts() {\n      this.onlinePanels = this.panels.filter(panel => panel.status === 'ativo').length;\n      this.offlinePanels = this.panels.filter(panel => panel.status !== 'ativo').length;\n    }\n  },\n  mounted() {\n    this.fetchPanelData();  // Chama os dados assim que o componente é montado\n  }\n};\n</script>\n\n<style scoped>\n/* Estilos... (sem alterações nos seus estilos anteriores) */\n</style>\n"],"mappings":"AAgCA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MAAA;MACAC,YAAA;MACAC,aAAA;MACAC,WAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,eAAA;MACA;QACA,MAAAC,QAAA,SAAAT,KAAA,CAAAU,IAAA;UACAC,cAAA;UACA;UACA,+FACA;UACA;UAAA;QAEA;;QAEA;QACA,MAAAC,OAAA,GAAAH,QAAA,CAAAP,IAAA;QACA,KAAAC,MAAA,GAAAS,OAAA,CAAAC,GAAA,CAAAC,MAAA;UACAC,IAAA,EAAAD,MAAA,CAAAC,IAAA;UACAC,MAAA,EAAAF,MAAA,CAAAE,MAAA;UAAA;UACAC,MAAA,EAAAH,MAAA,CAAAG;QACA;QACA,KAAAC,iBAAA;MACA,SAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,sCAAAA,KAAA;MACA;IACA;IACAD,kBAAA;MACA,KAAAd,YAAA,QAAAD,MAAA,CAAAkB,MAAA,CAAAC,KAAA,IAAAA,KAAA,CAAAL,MAAA,cAAAM,MAAA;MACA,KAAAlB,aAAA,QAAAF,MAAA,CAAAkB,MAAA,CAAAC,KAAA,IAAAA,KAAA,CAAAL,MAAA,cAAAM,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAhB,cAAA;EACA;AACA","ignoreList":[]}]}
